{
  "buildCommand": "npm install",
  "devCommand": "npm run dev",
  "framework": null,
  "public": true,
  "functions": {
    "server.js": {
      "runtime": "nodejs20.x"
    }
  },
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/server.js"
    }
  ],
  "headers": [
    {
      "source": "/json/:path*",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "https://glamorbybee.com, https://dev-admin.glamorbybee.com, https://admin.glamorbybee.com"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET, OPTIONS"
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "Content-Type"
        }
      ]
    }
  ]
}
